{
  "name": "mysimpleweather",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "lint": "ts-standard"
  },
  "dependencies": {
    "react": "17.0.0",
    "react-dom": "17.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@vitejs/plugin-react-refresh": "1.3.1",
    "husky": "7.0.1",
    "ts-standard": "10.0.0",
    "typescript": "4.3.2",
    "vite": "2.4.3"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/ts-standard/eslintrc.json"
    ],
    "ignorePatterns": "*.d.ts",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "ts-standard": {
    "ignore": [
      "*.d.ts",
      "vite.config.ts"
    ]
  }
}